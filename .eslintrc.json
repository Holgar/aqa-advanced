{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-plusplus": 0,
        "no-unused-expressions": 0,
        "func-names": 0,
        "no-restricted-syntax": 0,
        "no-underscore-dangle": 0,
        "import/extensions": 0
    }
}
